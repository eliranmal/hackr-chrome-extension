{
    "name": "hackr",
    "description": "a fun hacker simulator",
    "version": "0.1.1",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "js": [
            "lib/jquery/jquery-2.2.0.min.js",
            "lib/hackr/hackr.js",
            "app.js"
        ],
        "run_at": "document_end"
    }],
    "browser_action": {
        "default_title": "hackr()",
        "default_icon": "images/logo-48.png"
    },
    "icons": {
        "48": "images/logo-48.png"
    },
    "homepage_url": "https://jsfiddle.net/eliranmal/y696ro11/",
    "manifest_version": 2
}
